{
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [0, 4, { "SwitchCase": 1 }],
        "no-undef": [2],
        "semi": [2, "always"],
        "no-empty": [2, { "allowEmptyCatch": true }],
        "array-callback-return": 2,
        "block-scoped-var": 2,
        "no-alert": 2,
        "guard-for-in": 2,
        "no-caller": 2,
        "no-div-regex":2,
        "no-empty-function": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-script-url": 2,
        "no-implied-eval": 2,
        "no-extend-native": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-proto": 2,
        "no-self-compare": 2,
        "no-useless-call": 2,
        "no-unmodified-loop-condition": 2,
        "no-useless-concat": 2,
        "no-with": 2,
        "no-useless-return": 2,
        "eqeqeq": 2,
        "no-var": 2,
        "prefer-const": 2,
        "no-const-assign": 2,
        "linebreak-style": [2, "unix"],
        "quotes": [2, "single"],
        "no-duplicate-imports": 2
    }
}
