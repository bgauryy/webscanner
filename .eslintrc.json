{
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "rules": {
        "newline-after-var": 2,
        "import/no-cycle": 2,
        "import/newline-after-import": 2,
        "complexity": 2,
        "no-console": 2,
        "import/export": 2,
        "import/named": 2,
        "import/no-mutable-exports": 2,
        "no-trailing-spaces": 2,
        "no-spaced-func": 2,
        "import/no-unresolved": 2,
        "import/no-unused-modules": 2,
        "no-native-reassign": 2,
        "no-implicit-globals": 2,
        "no-invalid-this": 2,
        "no-global-assign": 2,
        "no-unexpected-multiline": 2,
        "no-multiple-empty-lines": 2,
        "eqeqeq": 2,
        "no-useless-escape": 2,
        "no-extend-native": 2,
        "no-multi-spaces": "error",
        "no-undef": 2,
        "semi": 2,
        "no-empty": 2,
        "array-callback-return": 2,
        "block-scoped-var": 2,
        "no-alert": 2,
        "guard-for-in": 2,
        "no-caller": 2,
        "no-div-regex": 2,
        "no-empty-function": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-script-url": 2,
        "no-implied-eval": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-proto": 2,
        "no-prototype-builtins": 2,
        "no-self-compare": 2,
        "no-useless-call": 2,
        "no-unmodified-loop-condition": 2,
        "no-useless-concat": 2,
        "no-with": 2,
        "no-useless-return": 2,
        "no-var": 2,
        "prefer-const": 2,
        "no-const-assign": 2,
        "linebreak-style": 2,
        "comma-spacing": 2,
        "vars-on-top": 2,
        "no-undefined": 2,
        "no-duplicate-imports": 2
    },
    "plugins": [
        "import"
    ]
}
